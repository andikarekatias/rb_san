<%= search_form_for(@query, url: search_path, method: :get,) do |f| %>
<div class="w3-row">
  <div class="w3-col s11">
    <%= f.search_field :daerah_or_nama_pelabuhan_i_cont_any, placeholder: "Search...", class: "w3-input w3-mobile" %>
  </div>
  <div class="w3-col s1">
    <%= button_tag(class: "w3-button w3-block w3-blue w3-mobile") do %>
      <i class='fa-solid fa-magnifying-glass'></i> 
    <% end %>

  </div>
</div>        
<% end %>